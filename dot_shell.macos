[ -d "/Applications/Postgres.app" ] && PATH="${PATH}:/Applications/Postgres.app/Contents/Versions/latest/bin"
[ -d "/Applications/Tailscale.app" ] && alias tailscale="/Applications/Tailscale.app/Contents/MacOS/Tailscale"

alias mac-start-check='ls /Library/StartupItems && ls /Library/LaunchDaemons && ls /System/Library/LaunchDaemons && ls ~/Library/LaunchAgents && ls /Library/LaunchAgents/ && ls /System/Library/LaunchAgents/'
